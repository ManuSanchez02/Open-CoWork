{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/**/*", "src/renderer/**/*.tsx", "src/renderer/**/*.ts"],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/*"],
      "@/*": ["src/renderer/*"]
    },
    "outDir": "out",
    "types": ["vite/client"]
  }
}
